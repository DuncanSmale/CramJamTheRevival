<% layout('layouts/boilerplate') -%>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Username</th>
      <th scope="col">Route</th>
      <th scope="col">Request Type</th>
      <th scope="col">Date</th>
      <th scope="col">User ID</th>
    </tr>
  </thead>
  <tbody>
    <% for(l of log) { %>
    <tr>
      <td><%= l.username %></td>
      <td><%= l.route %></td>
      <td><%= l.method %></td>
      <td><%= l.date.toDateString() %> <%= l.date.toTimeString() %></td>
      <td><%= l.userid %></td>
    </tr>
    <% } %>
  </tbody>
</table>
